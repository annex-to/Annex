{
  "name": "@annex/server",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./routers": "./src/routers/index.ts"
  },
  "scripts": {
    "dev": "bun --watch --env-file=.env src/index.ts",
    "build": "bunx prisma generate",
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "db:generate": "bunx prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:migrate:prod": "bunx prisma migrate deploy",
    "db:studio": "bunx prisma studio",
    "db:push": "bunx prisma db push"
  },
  "dependencies": {
    "@annex/shared": "workspace:*",
    "@prisma/client": "^6.1.0",
    "@trpc/server": "^10.45.2",
    "fast-xml-parser": "^5.3.2",
    "irc-framework": "^4.14.0",
    "parse-torrent-title": "^2.1.0",
    "ssh2-sftp-client": "^12.0.1",
    "superjson": "^2.2.2",
    "uuid": "^13.0.0",
    "webtorrent": "^2.8.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/uuid": "^11.0.0",
    "@types/webtorrent": "^0.110.1",
    "domhandler": "^5.0.3",
    "prisma": "^6.1.0",
    "typescript": "^5.7.2"
  }
}
